{
  "name": "visiojs",
  "version": "0.0.5",
  "type": "module",
  "types": "types/visiojs.d.ts",
  "description": "Microsoft Visio javascript alternative for creating diagrams and schematics",
  "keywords": ["visio", "schematic", "flowchart", "schematic", "schematics", "diagram", "svg"],
  "author": "Will Kelsey",
  "license": "MIT",
  "files": [
    "dist",
    "types"
  ],
   "repository": {
    "type": "git",
    "url": "https://github.com/28raining/visiojs.git"
  },
  "main": "./dist/visiojs.umd.cjs",
  "module": "./dist/visiojs.js",
  "unpkg": "./dist/visiojs.umd.js",
  "jsdelivr": "./dist/visiojs.umd.js",
  "exports": {
    ".": {
      "import": "./dist/visiojs.js",
      "require": "./dist/visiojs.umd.cjs"
    },
    "./dist/visiojs.css": "./dist/visiojs.css"
  },
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "preview": "vite preview",
    "lint": "eslint src",
    "prettier": "prettier --write src"
  },
  "devDependencies": {
    "@eslint/js": "^9.22.0",
    "eslint": "^9.26.0",
    "prettier": "^3.6.2",
    "vite": "^6.3.5"
  },
  "dependencies": {
    "d3": "^7.9.0"
  }
}
